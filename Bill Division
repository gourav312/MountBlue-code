void bonAppetit(vector<int> bill, int k, int b) {
    int sum =0;
    int n =bill.size();
    for(int i=0;i<n;i++){
        if(i!=k){
            sum+=bill[i];
        }
    }
    sum=sum/2;
    int diff=b-sum;
    if(diff==0){
        cout<<"Bon Appetit"<<endl;
    }else{
        cout<<diff<<endl;
    }
}
